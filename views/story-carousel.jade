#story-modal.modal.bs-modal-lg.fade(role='dialog', tabindex='-1', aria-labelledby='story-modal', aria-hidden='true')
    .modal-dialog
        .modal-content
            .modal-header
                button.close(type='button', data-dismiss='modal', aria-hidden='true') &times;
                h4.modal-title#story-modal-title Story

            .modal-body
                .video-container(style='padding-bottom: 56.25%')
                    iframe(width="640", height="340", src="", 
                           frameborder="0", allowfullscreen)